/**app.wxss**/
.container {
  padding: 0;
  box-sizing: border-box;
} 
/*adver  */
.adver{
  margin-top: 60rpx;
  position: fixed;
  width: 100%;
  bottom: 0;
}
.adver image{
    width: 100%;
    height: 200rpx;
    margin: 0 auto;
}
/*button  */
button:after{
    content: none;
}
button{
  padding: 0!important;
  margin: 0!important;
  line-height: inherit;
  background-color:#fff;

}
.resultShow{
  margin: 20rpx 0;
  display: flex;
  justify-content: space-around;
  position: relative;
}
.resultShow view image{
    width: 230rpx;
    height:30rpx;
}
.showLeft{
  position: absolute;
  top: 0rpx;
  left: 20rpx;
}
.showRight{
  position: absolute;
  top: 0rpx;
  right: 20rpx;
}